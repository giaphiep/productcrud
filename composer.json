{
    "name": "GiapHiep/Productcrud",
    "type": "library",
    "description": "Product CRUD",
    "keywords": [
        "GiapHiep",
        "Productcrud"
    ],
    "homepage": "https://github.com/giaphiep/productcrud",
    "license": "MIT",
    "authors": [
        {
            "name": "Giáp Hiệp",
            "email": "hiep.giapvan@gmail.com",
            "homepage": "https://giaphiep.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "php" : "~7.0",
        "yajra/laravel-datatables-oracle": "~8.0"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=5.4.3",
        "squizlabs/php_codesniffer": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "GiapHiep\\Productcrud\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GiapHiep\\Productcrud\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
	      "providers": [
	        "GiapHiep\\Productcrud\\ProductcrudServiceProvider"
	      ]
	    }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
